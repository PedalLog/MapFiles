# Except for explicitly tracked files in data/
!data/*.mbtiles

# Large source data files
data/sources/*.zip
data/sources/*.sqlite
data/sources/*.sqlite.zip

# Planetiler jar file (too large)
planetiler.jar

# Planetiler temporary files
data/tmp/
*.tmp

